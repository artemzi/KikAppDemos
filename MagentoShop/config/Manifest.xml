<?xml version="1.0" encoding="utf-8"?>
<manifest>
    <application generateAndroid="true" generateiOS="false">
        <main>shop.php</main>
        <services>http://demo.kikapptools.com/MagentoShop/services/</services>
    	<android navigationStyle="Slide" layoutOrientation="Portrait">   
    	    <Theme>AndroidTheme</Theme>     	
        	<versionCode>1</versionCode>
        	<versionName>1.0</versionName>
        	<packagedName>com.kikapp.magentoshop</packagedName>
        	<lunchImage>lunch.png</lunchImage>
        	<applicationIcon>appicon.png</applicationIcon>        	
        	<applicationTitle>Magento Shop</applicationTitle>
        	<!-- Android Maps API Key -->
        	<MapAPIKey/>            	
        	<!-- Adds -->
        	<EnableAds>False</EnableAds>
        	<AdsProvider></AdsProvider> <!-- AdMob - Mobfox - SmartAd -->
        	<AdMobPublisherId></AdMobPublisherId>
        	<!-- Analytics -->
        	<EnableAnalytics>False</EnableAnalytics>
        	<GoogleAnalyticsTrackerId></GoogleAnalyticsTrackerId>
        	<!-- Twitter -->
        	<TwitterAppKey></TwitterAppKey>
        	<TwitterAppSecret></TwitterAppSecret>
        	<!-- Facebook -->
        	<FacebookAppId></FacebookAppId>
        	<!-- YouTube -->
        	<AndroidYoutubeAPIKey></AndroidYoutubeAPIKey>        	
        	
        </android>
        <iOS navigationStyle="Default" layoutOrientation="Portrait">
            <Theme>iOSTheme</Theme>
        	<versionCode>1</versionCode>
        	<versionName>1.0</versionName>
        	<packagedName></packagedName>
        	<applicationIcon></applicationIcon>
        </iOS>
       </application>	
</manifest>